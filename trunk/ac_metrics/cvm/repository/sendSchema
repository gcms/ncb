  </thrownExceptions>
  <script>import static java.lang.String; import java.lang.Exception; import static cvm.ucm.handlers.exception.NoSessionException; import static cvm.ucm.handlers.exception.ControlSchemaNotSentException; import static cvm.ucm.handlers.exception.DataSchemaNotSentException; Exception exception = null; String sID = ncb.getSessionID(connectionID); try{ if(sID == null){ exception = new NoSessionException(sID); throw exception;}if(control_xcml !=null){ if(!ncb.sendSchema(sID, senderID, receiverID, control_xcml)){ exception = new ControlSchemaNotSentException(); throw exception;}} if(data_xcml != null){if(!ncb.sendSchema(sID, senderID, receiverID, data_xcml)){ exception = new DataSchemaNotSentException(); throw exception;}}}catch(Exception e){return e;} ucmNotifier.notifySendSchemaReply_Event(true); return exception;</script>
